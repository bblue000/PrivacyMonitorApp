<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/AppStyle.RootLayout">

    <RelativeLayout 
        android:id="@+id/feedback_title_rlt"
        style="@style/AppStyle.ContainerLayout.WrapContent"
        android:layout_height="@dimen/main_title_height"
        android:background="@color/appbase_blue">
        
        <TextView 
            android:id="@+id/activity_webview_title_tv"
            style="@style/AppStyle.Common.Title.Inverse"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="center"/>
        
    </RelativeLayout>
    
    <WebView 
        android:id="@+id/activity_webview_wv"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/feedback_title_rlt"/>
    
</RelativeLayout>
